{
  "name": "utils-nestjs",
  "version": "0.0.8-beta.2",
  "description": "Utils for nestjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "utils",
    "nestjs",
    "nest"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:tsup": "tsup",
    "lint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SashokNaSteroidah/utils-nestjs.git"
  },
  "author": "SashokNaSteroidah",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "@types/node": "20.12.12",
    "eslint": "8.57.0",
    "globals": "15.2.0",
    "terser": "^5.31.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0"
  },
  "peerDependencies": {
    "chalk": "5.3.0"
  }
}
